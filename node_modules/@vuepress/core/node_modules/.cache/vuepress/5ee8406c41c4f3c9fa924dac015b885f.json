mit('currentTag', tag)\r\n    }\r\n\r\n    const tagClick = (tagInfo) => {\r\n      if (instance.$route.path !== tagInfo.path) {\r\n        instance.$router.push({ path: tagInfo.path })\r\n      }\r\n    }\r\n\r\n    const paginationChange = (page) => {\r\n      setTimeout(() => {\r\n        window.scrollTo(0, 0)\r\n      }, 100)\r\n    }\r\n\r\n    return { posts, getCurrentTag, tagClick, paginationChange }\r\n  }\r\n})\r\n"]}]}